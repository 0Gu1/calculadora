class calculadora:
    def __init__(self,a,b) -> None:
        self.a=a
        self.b=b
    def somar(self):
        return self.a+self.b 
    def subtrair(self):
        return self.a-self.b   
    def multiplicar(self):
        return self.a*self.b
    def dividir(self):
        return self.a/self.b
    
c = calculadora(5,199)
print(c.somar()) #13   
print(c.dividir()) #0,gdg
print(c.multiplicar()) #40
print(c.subtrair()) #-3'
